{
  "name": "vod-pipeline",
  "version": "0.1.0",
  "description": "AI-Assisted Video Editor for Twitch VODs",
  "main": "dist/src/electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "pnpm setup:dev && concurrently \"pnpm dev:renderer\" \"wait-on http://localhost:5173 && pnpm dev:electron\"",
    "setup:dev": "pnpm rebuild && node ./scripts/copy-database.js",
    "dev:renderer": "vite",
    "dev:electron": "concurrently \"tsc -p tsconfig.electron.json --watch\" \"wait-on dist/src/electron/main.js dist/src/electron/preload.js && electron dist/src/electron/main.js\"",
    "dev:main": "electron dist/src/electron/main.js",
    "build": "pnpm build:renderer && pnpm build:electron",
    "build:renderer": "vite build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "dist": "pnpm build && electron-builder",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "echo \"Linting not configured yet\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "postinstall": "node ./scripts/install-ffmpeg.js"
  },
  "keywords": [
    "video",
    "editor",
    "electron",
    "twitch",
    "vod",
    "ai"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.26.0",
  "dependencies": {
    "@langchain/anthropic": "^1.3.12",
    "@langchain/core": "^1.1.17",
    "@langchain/google-genai": "^2.1.13",
    "@langchain/langgraph": "^1.1.2",
    "@langchain/langgraph-checkpoint-sqlite": "^1.0.0",
    "@langchain/openai": "^1.2.3",
    "better-sqlite3": "^12.6.2",
    "dotenv": "^17.2.3",
    "svelte": "^5.48.3",
    "uuid": "^13.0.0",
    "wavesurfer.js": "^7.12.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.1.0",
    "adm-zip": "^0.5.16",
    "concurrently": "^9.2.1",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "svelte-check": "^4.3.5",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^3.0.0",
    "wait-on": "^8.0.5"
  }
}
